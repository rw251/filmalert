  "scripts": {
    "build:dev": "rollup -c",
    "build:production": "rollup -c --environment BUILD:production",
    "deploy": "node deploy.mjs",
    "dev": "npx wrangler pages dev public/ --compatibility-flags=\"nodejs_compat\" --ip=0.0.0.0",
    "justdev": "npx wrangler pages dev public/ --compatibility-flags=\"nodejs_compat\" --ip=0.0.0.0",
    "predev": "npm run build:dev",
    "db:init": "npx wrangler d1 execute film --local --file \"./db/_schema.sql\"",
    "db:channels": "npx wrangler d1 execute film --local --command \"SELECT * FROM channels\""
  },
