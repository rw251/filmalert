{
  "name": "filmalert",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --port 9448",
    "build": "webpack --mode development",
    "build:production": "webpack --mode production",
    "predeploy": "npm version patch && npm run build:production",
    "deploy": "npm run deploy:client && npm run deploy:cron",
    "deploy:firebase": "cd src/firebase & firebase deploy --only functions",
    "predeploy:client": "npm run build:production",
    "deploy:client": "rsync -az --force --delete --progress -e 'ssh' ./dist/public_html/film/ rw251.com:~/public_html/film --exclude=/.ssh --exclude=/.logs --exclude=/.cagefs --exclude=/.cl.selector --exclude=/.bash_history",
    "deploy:cron": "rsync -az --force --delete --progress -e 'ssh' ./harvester/film/ rw251.com:~/cron/film --exclude=/.ssh --exclude=/.env --exclude=/.logs --exclude=/.cagefs --exclude=/.cl.selector --exclude=/.bash_history --exclude=/node_modules --chmod=Fu=rwx,Fog=rwx",
    "postdeploy": "git push --follow-tags",
    "preserve": "npm run build",
    "serve": "live-server ./public_html/cdown --port=7399 --entry-file=index.html --quiet --no-browser"
  },
  "keywords": [],
  "author": "Richard Williams <1234richardwilliams@gmail.com> (https://rw251.github.io)",
  "browserslist": [
    "ie 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.15.0",
    "firebase-tools": "^7.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "rollbar-deploy-webpack-plugin": "^1.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "firebase": "^6.2.0",
    "html-replace-webpack-plugin": "^2.5.5",
    "rollbar-sourcemap-webpack-plugin": "^2.5.0",
    "workbox-window": "^4.3.1"
  }
